            <div class="carousel-item active">
                <div class="carousel-bg" style="background-image: url('/Images/Weddiong.jpg');"></div>
                <div class="hero-content text-center">
                    @* <small class="d-inline-block fw-bold text-light text-uppercase bg-dark border border-light rounded-pill px-4 py-1 mb-4">
                        Welcome to Utsav
                    </small> *@
                    <h1 class="display-1 mb-4 text-white animated bounceInDown">
                        Looking for the Most Creative & Innovative Event Planners in Odisha?
                    </h1>
@*                     @Html.ActionLink("Book Now", "BookNow", "Home", null, new { @class = "btn btn-primary border-0 rounded-pill py-3 px-4 px-md-5 me-4" } )
 *@                   
                    @{
                        var isLoggedIn = Context.Session.GetInt32("UserId") != null;
                    }

                    @if (isLoggedIn)
                    {
                        <!-- User is logged in: Show 'Book Now' -->
                        <a asp-controller="User" asp-action="CreateBooking" class="btn btn-primary border-0 rounded-pill py-3 px-4 px-md-5 me-4">
