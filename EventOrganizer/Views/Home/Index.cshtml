                        <!-- User is not logged in: Show 'Login to Book' -->
                        <a asp-controller="Login" asp-action="Index" asp-route-returnUrl="/User/CreateBooking" class="btn btn-primary border-0 rounded-pill py-3 px-4 px-md-5">
                            Login to Book
                        </a>
                    }




                    <a href="/Home/KnowMore" class="btn btn-primary border-0 rounded-pill py-3 px-4 px-md-5">Know More</a>
                </div>
            </div>

            <div class="carousel-item">
                <div class="carousel-bg" style="background-image: url('/Images/image9.jpg');"></div>
                <div class="hero-content text-center">
                    <h1 class="display-1 text-white animated bounceInLeft">Make Your Event Memorable with Us!</h1>
                </div>
            </div>

            <div class="carousel-item">
